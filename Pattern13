class Pattern13 {
public static void main(String args[]){
  for(char i=65;i<=71;i++)
		 {
             for(char j=71;j>=i;j--)
			 {
				 System.out.print(" ");
				 
			 }
			 
			 for(char k=65;k<=i;k++)
			 {
				// System.out.print(" ");  
				 System.out.print(k+" ");
				
			 }
             System.out.println();

		 }
}


}